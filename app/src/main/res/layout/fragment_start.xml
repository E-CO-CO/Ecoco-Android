<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/bg_start"
        tools:context=".common.view.start.StartFragment">


    <com.google.android.material.button.MaterialButton
            android:paddingEnd="40dp"
            app:iconPadding="-20dp"
            app:iconTint="@color/black"
            android:padding="18dp"
            android:width="380dp"
            app:cornerRadius="6dp"
            android:fontFamily="@font/gmarket_sans"
            android:layout_marginBottom="40dp"
            app:icon="@drawable/ic_kakao_svg"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:backgroundTint="@color/btn_kakao_login_color"
            app:layout_constraintStart_toStartOf="parent"
            android:text="@string/btn_kakao_login_text"
            android:id="@+id/btn_kakao_login"
            android:layout_width="wrap_content"
            android:textSize="20sp"
            android:layout_height="wrap_content" />
</androidx.constraintlayout.widget.ConstraintLayout>